{
  "name": "news-ateliers",
  "version": "1.0.0",
  "description": "Application des nouveaux ateliers",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"./{src,__test-system__}/**/*.{ts,tsx}\" --fix",
    "lint:watch": "chokidar \"./{src,__test-system__}/**/*.{ts,tsx}\" -c \"eslint --fix {path}\"",
    "format": "prettier --write \"./{src,__test-system__}/**/*.{ts,tsx,css,md}\"",
    "format:watch": "onchange --verbose \"./{src,__test-system__}/**/*.{ts,tsx,css,md}\" -- prettier --write --loglevel error {{changed}}",
    "test": "vitest run",
    "test:changed": "onchange \"./{src,__test-system__}/**/*.{ts,tsx}\" -d 2000 -- npm test",
    "preview": "vite preview",
    "system:watch": "tsx src/__test-system__/scripts/watch.ts",
    "start:dev": "concurrently \"npm run dev\" \"npm run lint:watch\" \"npm run format:watch\" \"npm run test:changed\" \"npm run system:watch\" --names \"DEV,LINT,FMT,TEST,SYS\" --prefix-colors \"blue,yellow,green,magenta,cyan\" --kill-others-on-fail --restart-after-error",
    "start:watch": "tsx src/__test-system__/scripts/start-project.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.4",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "chokidar": "^4.0.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "onchange": "^7.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-node": "^3.0.6",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "ISC"
}
